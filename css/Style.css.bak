.module .container {
	display: flex;
	justify-content: center;
	align-items: center;
	position: absolute;
	bottom: 0;
	top: 0;
	left: 0;
	right: 0;
}
.mid h1,
.mid p,
{
	font-weight: 400;
	color: white;
	text-transform: uppercase;
	text-align: center;
	font-size: 2rem;
	transform: translate(-50%, -50%);
}
.scrollup {
	width: 40px;
	height: 40px;
	position: fixed;
	bottom: 50px;
	right: 10px;
	z-index: 99999;
	display: none;
	background-color: #000;
	border-radius: 3px;
	color: #ffffff;
	padding: 7px 12px 12px 12px;
}
