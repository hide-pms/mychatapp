.slide-top p{ 
	 
	display: block;
  display: -webkit-box;
  max-height: 200px;
  height: $font-size*$line-height*$lines-to-show; 
	margin: 0 auto;
	font-size: 14px;
	line-height: 1.9;
	-webkit-line-clamp:4;
	
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
	margin-bottom: 20px;
}